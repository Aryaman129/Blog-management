{
  "name": "mosaic-paths-backend",
  "version": "1.0.0",
  "description": "Backend API for Mosaic Paths portfolio and blog platform",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "npm run db:generate && npm run db:deploy && tsc",
    "build:production": "npm install && npm run db:generate && npm run db:deploy && npm run db:seed:prod && tsc",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:deploy": "prisma db push --accept-data-loss",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node --project tsconfig.seed.json prisma/seed.ts",
    "db:seed:prod": "node -r ts-node/register --project tsconfig.seed.json prisma/seed.ts",
    "db:studio": "prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["express", "nodejs", "typescript", "prisma", "portfolio", "blog"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
}